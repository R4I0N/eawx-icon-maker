<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="WindowBackgroundColor">#1E1E1E</Color>
    <Color x:Key="ForegroundColor">White</Color>
    <SolidColorBrush x:Key="WindowBackground" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="TextForeground" Color="{StaticResource ForegroundColor}"/>

</ResourceDictionary>